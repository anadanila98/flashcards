{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "types": ["node"],
    "lib": ["ES2020"],
    "noEmit": true
  },
  "include": ["vite.config.ts"]
}
